{
    "name": "swooliy/swooliy-lumen",
    "description": "A lumen package that uses swoole instead of php-fpm to manage and speed up the lumen application",
    "keywords": [
        "package",
        "lumen",
        "artisan",
        "console command",
        "swoole",
        "fast api service"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ney Chang",
            "email": "zoobile@gmail.com",
            "homepage": "https://github.com/zairl23"
        }
    ],
    "require": {
        "ext-swoole": "*",
        "swooliy/server": "*"
    },
    "autoload": {
        "psr-4": {
            "Swooliy\\Lumen\\": "./src/Swooliy/Lumen/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swooliy\\Lumen\\SwooliyServiceProvider"
            ]
        }
    }
}

